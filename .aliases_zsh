echo $shell

alias a=alias
a zs="source ~/.zshrc"

# Directory
a u="cd .."
a uu="cd ../.."
a uuu="cd ../../.."

# Listing
a ll="ls -al"
a ltr="ls -ltr"

# Grepping
a grep="grep --color=auto"
a fgrep="fgrep --color=auto"
a egrep="egrep --color=auto"

a diff="diff --color=auto"

# Shortcut
a wk="cd ~/Documents/workspace"

# Git
alias gs='git status '
alias ga='git add '
alias grm='git rm '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '
#alias gk='gitk --all&'
#alias gx='gitx --all'

alias got='git '
alias get='git '

alias multipull="find . -mindepth 1 -maxdepth 1 -type d -print -exec git -C {} pull \;"
#EOF
